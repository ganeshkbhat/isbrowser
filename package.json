{
  "name": "which-platform",
  "version": "1.0.0",
  "description": "check if an enviromment is a nodejs or browser enviroment",
  "main": "index.js",
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "unpkg": "index.min.js",
  "scripts": {
    "test": "mocha --reporter spec --recursive --timeout 60000",
    "browserify": "browserify index.js > browser.js",
    "uglify": "uglifyjs --compress --mangle reserved=['$','require','exports'] index.js -o index.min.js",
    "start": "lite-server"
  },
  "directories": {
    "test": "test"
  },
  "pre-commit": [
    "uglify",
    "test"
  ],
  "devDependencies": {
    "@types/node": "^20.11.16",
    "chai": "^4.4.1",
    "live-server": "^1.2.2",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "unimported": "^1.31.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshkbhat/isbrowser.git"
  },
  "bugs": {
    "url": "https://github.com/ganeshkbhat/isbrowser/issues"
  },
  "homepage": "https://github.com/ganeshkbhat/isbrowser.git#readme",
  "keywords": [
    "is browser environment",
    "is nodejs environment",
    "isbrowser environment",
    "check if an enviromment is a nodejs enviroment",
    "check if an enviromment is a browser enviroment"
  ],
  "author": "Ganesh B <ganeshsurfs@gmail.com>",
  "license": "MIT"
}